
 Complex(dp) Function cTrace(A)
 Implicit None

  Complex(dp), Intent(in) :: A(:,:)

  Integer :: n, i1

  cTrace = 0._dp
  n = Size(a, dim=1)
  Do i1=1,n
   cTrace = cTrace + A(i1,i1)
  End Do

 End Function cTrace

Real(dp) Function dTrace(A)
 Implicit None

  Real(dp), Intent(in) :: A(:,:)

  Integer :: n, i1

  dTrace = 0._dp
  n = Size(a, dim=1)
  Do i1=1,n
   dTrace = dTrace + A(i1,i1)
  End Do

 End Function dTrace